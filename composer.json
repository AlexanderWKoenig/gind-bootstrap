{    
    "name": "gind/gind-bootstrap",
    "type": "typo3-cms-extension",
    "description": "Extends bootstrap-package",
    "keywords": [
        "TYPO3",
        "extension"
    ],
    "homepage": "https://gindserver.duckdns.org",
    "authors": [
        {
            "name": "Alexander KÃ¶nig",
            "role": "Developer"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": ">=10.4 <11.9.99",
        "bk2k/bootstrap-package": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Gind\\GindBootstrap\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/gind-bootstrap": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "gind_bootstrap",
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }

}
